{
  "name": "Donations Management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-ai-sdk/orchestration": "^2.3.0",
    "@sap-cloud-sdk/http-client": "^4.2.0",
    "@sap-cloud-sdk/util": "^4.2.0",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.12.0",
    "dotenv": "^16.6.1",
    "express": "^4",
    "hdb": "^2.26.3",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve && node -r dotenv/config srv/server.js",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-um_donations_management.donationsmanagement": "cds watch --open um_donations_management.donationsmanagement/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/um_donations_management.donationsmanagement"
  ]
}
